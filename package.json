{
  "author": "Artem Polyakov",
  "name": "pug-sass-webpack-template",
  "version": "1.0.0",
  "description": "",
  "homepage": "",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "scripts": {
    "build": "webpack --config config/webpack.dev.config.js --mode=development",
    "watch": "webpack --config config/webpack.dev.config.js --mode=development --watch",
    "bundle": "npm install && npm run watch",
    "dev": "webpack serve --config config/webpack.dev.config.js --mode=development",
    "production": "webpack --config config/webpack.prod.config.js --mode=production --stats-children",
    "lint:sass": "npx stylelint src/**/*.scss --fix",
    "lint:sass-fix": "npx stylelint src/**/*.scss --fix",
    "lint:js": "eslint src/",
    "lint:js-fix": "eslint src/ --fix",
    "stats": "webpack --config config/webpack.prod.config.js --mode=production --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.13.12",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "mini-css-extract-plugin": "^1.4.0",
    "node-static": "^0.7.11",
    "postcss": "^8.2.9",
    "postcss-loader": "^4.1.0",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "rimraf": "^3.0.0",
    "sass": "^1.32.8",
